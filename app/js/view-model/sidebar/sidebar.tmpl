<h4 class="filter-container__title">Shopping Options</h4>
<div class="item-container filter-container__content">
    <h5 class="filter-container__section-title">
        <i class="fa fa-usd"></i>
        Price
    </h5>
    <div class="filter-container__items-wrapper">
        <p class="filter-container__item">
            <input data-bind="checked: sidebar.priceFilter" class="filled-in" type="checkbox" value="0-20"
                   id="price-before-20">
            <label for="price-before-20" class="layered-nav-filter-label">Before $20</label>
        </p>
        <p class="filter-container__item">
            <input data-bind="checked: sidebar.priceFilter" class="filled-in" type="checkbox" value="20-30"
                   id="price-20-30">
            <label for="price-20-30" class="layered-nav-filter-label">$20-$30</label>
        </p>
        <p class="filter-container__item">
            <input data-bind="checked: sidebar.priceFilter" class="filled-in" type="checkbox" value="30-50"
                   id="price-30-50">
            <label for="price-30-50" class="layered-nav-filter-label">$30-$50</label>

        </p>
        <p class="filter-container__item">
            <input data-bind="checked: sidebar.priceFilter" class="filled-in" type="checkbox" value="50-Infinity"
                   id="price-50-above">
            <label for="price-50-above" class="layered-nav-filter-label">$50 &amp; Above</label>
        </p>
    </div>
</div>
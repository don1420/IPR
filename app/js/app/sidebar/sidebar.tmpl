<h4>Shopping Options</h4>
<div>
	<h4>Price</h4>
	<ol>
		<li>
			<div class="checkbox">
				<label for="price-before-20" class="layered-nav-filter-label">
				<input data-bind="checked: sidebar.priceFilter" class="checkbox" type="checkbox" value="0-20" id="price-before-20" onchange="">
					Before $20
				</label>
			</div>
		</li>
		<li>
			<div class="checkbox">
				<label for="price-20-30" class="layered-nav-filter-label">
					<input data-bind="checked: sidebar.priceFilter" class="checkbox" type="checkbox" value="20-30" id="price-20-30" onchange="">
					$20-$30
				</label>
			</div>
		</li>
		<li>
			<div class="checkbox">
				<label for="price-30-50" class="layered-nav-filter-label">
					<input data-bind="checked: sidebar.priceFilter" class="checkbox" type="checkbox" value="30-50" id="price-30-50" onchange="">
					$30-$50
				</label>
			</div>
		</li>
		<li>
			<div class="checkbox">
				<label for="price-50-above" class="layered-nav-filter-label">
					<input data-bind="checked: sidebar.priceFilter" class="checkbox" type="checkbox" value="50-Infinity" id="price-50-above" onchange="">
					$50 &amp; Above
				</label>
			</div>
		</li>
	</ol>
</div>